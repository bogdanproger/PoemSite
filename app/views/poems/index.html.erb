<h1>Poems</h1>
<% @poems.each do |poem| %>
  <h2><%= poem.title%></h2>
  <p><%= poem.body%></p>
  <br>
  <%= link_to 'Edit', edit_poem_path(poem.id) %> |
  <%= link_to 'Destroy', poem_path(poem.id), data: {method: :delete, confirm: 'Are you sure?'} %>
  <%= link_to 'Show', poem_path(poem.id) %> |
<% end %>